#tag Events popBarID
	#tag Event
		Function MouseDown(X As Integer, Y As Integer) As Boolean
		  dim allBars() as dictionary = demo.getAllBarsData
		  
		  me.DeleteAllRows
		  
		  for each bar as dictionary in allBars
		    me.AddRow(bar.value("ID"))
		  next
		End Function
	#tag EndEvent
	#tag Event
		Sub Change()
		  cntBarProperties.Init demo, me.SelectedRowValue
		End Sub
	#tag EndEvent
#tag EndEvents
