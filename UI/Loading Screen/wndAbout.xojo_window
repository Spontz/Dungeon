#tag Events lblAppName
	#tag Event
		Sub Open()
		  dim d as new date
		  dim year as integer
		  
		  if d.Year < 2019 then
		    year = 2019
		  else
		    year = d.Year
		  end if
		  
		  me.Text = me.Text + str(Year)
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events lblVersion
	#tag Event
		Sub Open()
		  me.text = "Version " + str(App.MajorVersion) + "." + str(App.MinorVersion) + "." + str(App.BugVersion) + "." + str(App.StageCode) + "." + str(App.NonReleaseVersion)
		End Sub
	#tag EndEvent
#tag EndEvents
