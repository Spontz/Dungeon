; Specify the model to load and the shader to applysModelFilePath /pool/models/nanosuit/nanosuit.objsShaderPath /pool/models/nanosuit/nanosuitLights.glslfEnableDepthBufferClearing 0fDisableDepthTest 0fDisableDepthMask 0fEnableDrawWireframe 0; Animation support for objects, bones and camerasfEnableAnimation 0fAnimationNumber 0; Wich camera to use? (-1 means no camera used)string CameraNumber:=-1;; Animation time (normally 't')string aTime:=0;; ******************; OBJECT POSITIONING; ******************; Translation for object nstring tx:=0; ty:=-6; tz:=-3;; Rotation for object nstring rx:=0; ry:=180*sin(t); rz:=0;; Scale for object nstring sx:=0.5; sy:=0.5; sz:=0.5;; ********************************************************************************; SHADER VARIABLES; Any other shader uniform variables used in the shader, should be configured here; ********************************************************************************uniform float ambientStrength v1:=0.1;uniform float specularStrength v1:=0.5+2*beat;uniform vec3 lightPos v1:=3*sin(t);v2:=2*cos(t);v3:=1; uniform vec3 lightColor v1:=1;v2:=1;v3:=1;uniform vec3 viewPos v1:=cam_posX;v2:=cam_posY;v3:=cam_posZ; 