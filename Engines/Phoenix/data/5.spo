[fbobind]
id 5
start 0
end 100
enabled 1
layer 5
blend ONE ONE

; Stores the buffer in the video
; card memory as a texture,
; for a later use

; Integer representing the FBO texture unit
fFboUnit 0

; Clear the screen buffer after capturing? (1=yes, 0=no)
fClearScreenBuffer 1

; Clear the depth buffer after capturing? (1=yes, 0=no)
fClearDepthBuffer 1