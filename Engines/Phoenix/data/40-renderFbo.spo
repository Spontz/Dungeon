[renderFbo]
id 40
start 0
end 100
enabled 1
layer 40
blend ONE ONE
blendequation ADD

; Recovers a previously saved screen buffer from the video
; card and puts it on the screen with the desired parameters

; Integer representing the fbo unit that will be
; recovered over the current screen contents
fFboNum 0

; Clear the screen buffer before restoring? (1=yes, 0=no)
fClearScreenBuffer 0

; Clear the depth buffer after restoring? (1=yes, 0=no)
fClearDepthBuffer 1